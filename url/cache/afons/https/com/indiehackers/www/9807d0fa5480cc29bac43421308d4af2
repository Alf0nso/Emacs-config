window.ih_bc = function () {
  const l = window.location;
  let q = l.search || '';
  if (q.indexOf('nocache=') === -1) {
    q = q ? q + `&nocache=${+new Date()}` : `?nocache=${+new Date()}`;
    location = l.origin + l.pathname + q;
  }
};

// clean query params
window.ih_cqp = function () {
  const l = window.location;
  const h = window.history;
  let q = l.search || '';
  const m = q.match(/nocache=[^&]+&?/);
  if (m && h && h.replaceState) {
    q = q.replace(m, '').replace(/^\?$/, '').replace(/&$/, '');
    const u = l.origin + l.pathname + q;
    h.replaceState({ path: u }, '', u);
  }
